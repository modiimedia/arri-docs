    "tabWidth": 4,
    "trailingComma": "all",
    "useTabs": false,
    "singleQuote": true,
    "semi": true,
    "endOfLine": "lf",
    "printWidth": 80,
    "plugins": ["prettier-plugin-tailwindcss"],
    "overrides": [
        {
            "files": ["*.json", "*.yaml"],
            "options": {
                "tabWidth": 2
            }
        }
    ]
}
